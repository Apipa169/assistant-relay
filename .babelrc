{
  "presets": ["next/babel", "@babel/preset-env"],
  "plugins": [
    ["import", { "libraryName": "antd" }],
    ["babel-plugin-root-import",
      {
        "rootPathSuffix": "./",
        "rootPathPrefix": "~/"
      }
    ],
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-proposal-nullish-coalescing-operator"
  ]
}
